{
  "name": "jsramverk-editor",
  "version": "2.3.1",
  "description": "jsramverk editor built with MERN stack",
  "main": "index.js",
  "scripts": {
    "test": "jest ./me/jsramverk-backend/text-editor-backend --detectOpenHandles --coverage --config ./backend/jest.config.json  --testTimeout=10000",
    "start": "node ./me/jsramverk-backend/text-editor-backend/server.js",
    "server": "nodemon ./me/jsramverk-backend/text-editor-backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "mongoose",
    "mongodb"
  ],
  "author": "Rezaee",
  "license": "ISC",

  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "jest": "^29.3.1"
  }
}
